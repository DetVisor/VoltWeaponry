  <ThingDef ParentName="BaseHumanMakeableGun">
    <techLevel>Industrial</techLevel>
    <defName>DV_Gun_VoltRifle</defName>
    <label>volt rifle</label>
    <description>A simple, single-fire lens array rifle. It fires beams of compressed photons that sear flesh. Needs time to cool down between each shot, but it can make tighter beams, giving it a better range. Ineffective against any armor.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/WeaponRanged/BeamRifle</texPath>
      <drawSize>1.1</drawSize>
      <graphicClass>Graphic_Single</graphicClass>
      <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <statBases>
      <WorkToMake>41000</WorkToMake>
      <Mass>3</Mass>
      <AccuracyTouch>0.55</AccuracyTouch>
      <AccuracyShort>0.64</AccuracyShort>
      <AccuracyMedium>0.69</AccuracyMedium>
      <AccuracyLong>0.74</AccuracyLong>
      <RangedWeapon_Cooldown>1.4</RangedWeapon_Cooldown>
    </statBases>
    <recipeMaker>
      <researchPrerequisite>DV_LensArrays</researchPrerequisite>
      <recipeUsers Inherit="False">
        <li>TableMachining</li>
      </recipeUsers>
      <displayPriority>180</displayPriority>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <costList>
      <Jade>5</Jade>
      <Steel>40</Steel>
      <ComponentIndustrial>5</ComponentIndustrial>
    </costList>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <tradeTags>
      <li>WeaponRanged</li>
    </tradeTags>
    <weaponClasses>
      <li>LongShots</li>
    </weaponClasses>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>DV_VoltProj_Rifle</defaultProjectile>
        <warmupTime>1.8</warmupTime>
        <range>35.9</range>
        <soundCast>DV_Shot_WattRifle</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>1.6</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>

